<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_bookedtables"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@drawable/background"
    tools:context="com.example.dalal.gpnew.bookedtables">

    <ImageButton
        android:onClick="Back"
        android:layout_width="50dp"
        android:layout_height="70dp"
        android:background="@drawable/arow"
        android:id="@+id/imageButton2"
        android:layout_alignBottom="@+id/imageView"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="200sp"
        android:layout_height="50sp"
        android:background="@drawable/zwara_word"
        android:id="@+id/imageView"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />


    <ImageButton
        android:onClick="profile"
        android:layout_width="80dp"
        android:layout_height="71dp"
        tools:layout_editor_absoluteY="71dp"
        tools:layout_editor_absoluteX="0dp"
        android:id="@+id/imageButton3"
        android:scaleType="fitXY"
        android:background="@drawable/pers"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignBottom="@+id/imageView" />


    <ImageView
        android:layout_width="1100sp"
        android:layout_height="50sp"
        android:background="@drawable/picture3"
        tools:layout_editor_absoluteY="168dp"
        tools:layout_editor_absoluteX="2dp"
        android:id="@+id/imageView10"
        android:layout_below="@+id/imageView11"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="18dp" />

    <ImageView
        android:layout_width="1100sp"
        android:layout_height="50sp"
        android:background="@drawable/picture3"
        tools:layout_editor_absoluteY="260dp"
        tools:layout_editor_absoluteX="2dp"
        android:id="@+id/imageView11"
        android:layout_marginTop="90dp"
        android:layout_below="@+id/imageView"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />



    <TextView
        android:text=" طاولة رقم  77"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteY="285dp"
        tools:layout_editor_absoluteX="217dp"
        android:id="@+id/textView7"
        android:textColor="#7a5a23"
        android:layout_marginBottom="15dp"
        android:layout_alignBottom="@+id/imageView10"
        android:layout_toLeftOf="@+id/imageView3"
        android:layout_toStartOf="@+id/imageView3" />

    <TextView
        android:text="طاولة رقم  13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteY="193dp"
        tools:layout_editor_absoluteX="225dp"
        android:id="@+id/textView"
        android:textColor="#7a5a23"
        android:layout_marginBottom="12dp"
        android:layout_alignBottom="@+id/imageView11"
        android:layout_alignLeft="@+id/textView7"
        android:layout_alignStart="@+id/textView7" />

    <ImageButton
        android:layout_width="109dp"
        app:srcCompat="@drawable/confirmed"
        tools:layout_editor_absoluteY="278dp"
        tools:layout_editor_absoluteX="2dp"
        android:id="@+id/imageButton8"
        android:scaleType="fitXY"
        android:background="@android:color/transparent"
        android:layout_height="44dp"
        android:layout_above="@+id/imageView10"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/imageView11" />


    <TextView
        android:id="@+id/textView2"
        android:layout_width="213dp"
        android:layout_height="44dp"
        android:layout_marginTop="19dp"
        android:foreground="@android:color/transparent"
        android:text="الطاولات المحجوزة "
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textColor="#eeb44e"
        android:textSize="30dp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="67dp"
        tools:layout_editor_absoluteY="93dp"
        android:layout_below="@+id/imageButton2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageView
        android:layout_width="449dp"
        android:layout_height="50dp"
        app:srcCompat="@drawable/reserved"
        android:id="@+id/imageView7"
        android:layout_alignBottom="@+id/imageView10"
        android:layout_alignRight="@+id/imageButton8"
        android:layout_alignEnd="@+id/imageButton8" />

</RelativeLayout>
