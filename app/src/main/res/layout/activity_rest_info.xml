<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_rest_info"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@drawable/background"
    tools:context="com.example.dalal.gpnew.restInfo"><![CDATA[

    " />



    ]]>

    <Button
        android:text=""
        android:background="@drawable/regester"
        android:layout_width="wrap_content"
        android:layout_height="60sp"
        android:id="@+id/button15"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="27dp" />


    <EditText
        android:layout_width="100sp"
        android:layout_height="60sp"
        android:inputType="textPersonName"
        android:text="رقم الهاتف"
        android:ems="10"
        android:id="@+id/editText6"
        android:textAlignment="center"
        android:textColor="#A49279"
        android:layout_above="@+id/button15"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />


    <ImageView
        android:layout_width="227dp"
        android:layout_height="49dp"
        android:background="@drawable/zwara_word"
        android:id="@+id/imageView"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />


    <ImageButton
        android:onClick="GoProfilePage"
        android:layout_width="80dp"
        android:layout_height="71dp"
        tools:layout_editor_absoluteY="71dp"
        tools:layout_editor_absoluteX="0dp"
        android:id="@+id/imageButton3"
        android:scaleType="fitXY"
        android:background="@drawable/pers"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignBottom="@+id/imageView" />

    <ImageView
        android:layout_width="50dp"
        android:layout_height="70dp"
        android:background="@drawable/phone"
        android:id="@+id/imageView8"
        android:layout_above="@+id/button15"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:text="الموقع"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView39"
        android:textStyle="normal|bold"
        android:textSize="20sp"
        android:textColor="#7a5a23"
        android:layout_centerVertical="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@drawable/user_info"
        android:id="@+id/imageView7"
        android:layout_above="@+id/textView39"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageView
        android:layout_width="50dp"
        android:layout_height="60dp"
        android:background="@drawable/passicon"
        android:id="@+id/imageView6"
        android:layout_above="@+id/editText7"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="60sp"
        android:textColor="#A49279"
        android:textAlignment="center"
        android:inputType="textPassword"
        android:text="كلمة المرور"
        android:ems="10"
        android:id="@+id/editText8"
        android:layout_above="@+id/editText7"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="60sp"
        android:inputType="textPersonName"
        android:text="معلومات عن المطعم"
        android:ems="10"
        android:id="@+id/editText7"
        android:textAlignment="center"
        android:textColor="#A49279"
        android:layout_alignBottom="@+id/imageView7"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageView
        android:layout_width="50dp"
        android:layout_height="60dp"
        android:background="@drawable/kshk"
        android:id="@+id/imageView5"
        android:layout_above="@+id/editText8"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="60sp"
        android:inputType="textPersonName"
        android:text="اسم المطعم"
        android:textColor="#A49279"
        android:ems="10"
        android:id="@+id/editText5"
        android:textAlignment="center"
        android:layout_above="@+id/editText8"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageButton
        android:onClick="Back"
        android:layout_width="50dp"
        android:layout_height="70dp"
        android:background="@drawable/arow"
        android:id="@+id/imageButton2"
        android:layout_alignBottom="@+id/imageView"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

</RelativeLayout>
