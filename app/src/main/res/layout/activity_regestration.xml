<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_regestration"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@drawable/background"
    tools:context="com.example.dalal.gpnew.regestration">



    <TextView

        android:text=""
        android:onClick="Back"
        android:background="@drawable/arow"
        android:layout_width="40sp"
        android:layout_height="35sp"
        android:id="@+id/textView30"
        android:layout_weight="0.75"
        android:layout_toLeftOf="@+id/textView34"
        android:layout_alignBottom="@+id/textView34"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:text=""
        android:layout_width="200sp"
        android:layout_height="50sp"
        android:background="@drawable/zwara_word"
        android:id="@+id/textView34"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <Button
        android:text=""
        android:background="@drawable/regester"
        android:layout_width="wrap_content"
        android:layout_height="60sp"
        android:id="@+id/button15"
        android:layout_alignParentBottom="true"

        android:layout_marginBottom="27dp" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="60sp"
        android:inputType="textPersonName"
        android:text="معلومات عن المستخدم"
        android:ems="10"
        android:id="@+id/editText7"
        android:layout_below="@+id/editText8"
        android:layout_marginTop="15dp"
        android:textAlignment="center"
        android:textColor="#A49279"
        android:layout_alignRight="@+id/imageView2"
        android:layout_alignEnd="@+id/imageView2"
        android:layout_alignLeft="@+id/editText8"
        android:layout_alignStart="@+id/editText8" />

    <EditText
        android:layout_width="100sp"

        android:layout_height="60sp"
        android:inputType="textPersonName"
        android:text="رقم الهاتف"
        android:ems="10"
        android:id="@+id/editText6"
        android:textAlignment="center"
        android:layout_marginTop="19dp"
        android:textColor="#A49279"
        android:layout_below="@+id/editText7"
        android:layout_alignRight="@+id/imageView4"
        android:layout_alignEnd="@+id/imageView4"
        android:layout_alignLeft="@+id/editText7"
        android:layout_alignStart="@+id/editText7" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="60sp"
        android:inputType="textPersonName"
        android:text="اسم المستخدم"
        android:ems="10"
        android:id="@+id/editText5"
        android:textAlignment="center"
        android:layout_marginTop="14dp"
        android:textColor="#A49279"
        android:layout_alignTop="@+id/imageView"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/imageView"
        android:layout_alignEnd="@+id/imageView" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
       android:background="@drawable/persone2"
        android:id="@+id/imageView"
        android:layout_marginTop="25dp"
        android:layout_below="@+id/textView34"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="16dp"
        android:layout_marginEnd="16dp" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="60sp"
        android:textColor="#A49279"
        android:textAlignment="center"
        android:inputType="textPassword"
        android:text="كلمة المرور"

        android:ems="10"
        android:id="@+id/editText8"

        android:layout_below="@+id/imageView"
        android:layout_alignRight="@+id/imageView2"
        android:layout_alignEnd="@+id/imageView2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
       android:background="@drawable/password2"
        android:id="@+id/imageView2"
        android:layout_above="@+id/imageView3"
        android:layout_alignLeft="@+id/imageView"
        android:layout_alignStart="@+id/imageView" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/user_info"
        android:id="@+id/imageView3"
        android:layout_alignTop="@+id/editText7"
        android:layout_alignRight="@+id/editText7"
        android:layout_alignEnd="@+id/editText7"
        android:layout_alignLeft="@+id/imageView2"
        android:layout_alignStart="@+id/imageView2" />

    <TextView
        android:text=""
        android:onClick="GoProfilePage"
        android:layout_width="40sp"
        android:background="@drawable/person"
        android:layout_height="35sp"
        android:id="@+id/textView29"
        android:layout_weight="1"
        android:layout_alignParentTop="true"
        android:layout_alignRight="@+id/imageView"
        android:layout_alignEnd="@+id/imageView"
        android:layout_toRightOf="@+id/textView34"
        android:layout_toEndOf="@+id/textView34"
        android:layout_above="@+id/imageView" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
       android:background="@drawable/phone"
        android:id="@+id/imageView4"
        android:layout_alignTop="@+id/editText6"
        android:layout_alignBottom="@+id/editText6"
        android:layout_alignRight="@+id/imageView3"
        android:layout_alignEnd="@+id/imageView3" />

</RelativeLayout>
