<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_invitatonssaved"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@drawable/background"
    tools:context="com.example.dalal.gpnew.invitatonssaved">



    <ImageButton
        android:onClick="Back"
        android:layout_width="50dp"
        android:layout_height="70dp"
        android:background="@drawable/arow"
        android:id="@+id/imageButton2"
        android:layout_alignBottom="@+id/imageView"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="227dp"
        android:layout_height="49dp"
        android:background="@drawable/zwara_word"
        android:id="@+id/imageView"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />


    <ImageButton
        android:layout_width="80dp"
        android:layout_height="71dp"
        tools:layout_editor_absoluteY="71dp"
        tools:layout_editor_absoluteX="0dp"
        android:id="@+id/imageButton3"
        android:scaleType="fitXY"
        android:background="@drawable/pers"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignBottom="@+id/imageView" />


    <ImageView
        android:layout_width="1100sp"
        android:layout_height="50sp"
        android:background="@drawable/picture3"
        tools:layout_editor_absoluteY="172dp"
        tools:layout_editor_absoluteX="0dp"
        android:id="@+id/imageView10"
        android:layout_below="@+id/textView2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="34dp"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />


    <TextView
        android:text="حفل تخرجي"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteY="197dp"
        tools:layout_editor_absoluteX="241dp"
        android:id="@+id/textView"
        android:textColor="#7a5a23"
        android:layout_marginTop="16dp"
        android:layout_alignTop="@+id/imageView10"
        android:layout_alignLeft="@+id/imageButton3"
        android:layout_alignStart="@+id/imageButton3" />


    <ImageButton
        android:onClick="frindslist"
        tools:layout_editor_absoluteY="185dp"
        tools:layout_editor_absoluteX="1dp"
        android:id="@+id/imageButton12"
        android:scaleType="fitXY"
        android:layout_width="65dp"
        android:layout_height="55dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="@drawable/send"
        android:layout_alignTop="@+id/imageButton8"
        android:layout_alignBottom="@+id/imageButton8" />

    <ImageButton
        android:layout_width="65dp"
        tools:layout_editor_absoluteY="185dp"
        tools:layout_editor_absoluteX="71dp"
        android:id="@+id/imageButton8"
        android:scaleType="fitXY"
        android:layout_height="55dp"
        android:background="@drawable/edit"
        android:layout_alignTop="@+id/imageButton7"
        android:layout_toRightOf="@+id/imageButton12"
        android:layout_toEndOf="@+id/imageButton12"
        android:layout_alignBottom="@+id/imageView10" />

    <ImageButton
        tools:layout_editor_absoluteY="185dp"
        tools:layout_editor_absoluteX="142dp"
        android:id="@+id/imageButton7"
        android:scaleType="fitXY"
        android:layout_width="65dp"
        android:layout_height="55dp"
        android:background="@drawable/cancel"
        android:layout_toRightOf="@+id/imageButton8"
        android:layout_toEndOf="@+id/imageButton8"
        android:layout_alignBottom="@+id/imageView10"
        android:layout_alignTop="@+id/imageView10" />

    <TextView
        android:text="دعواتي المحفوظة"
        android:textColor="#eeb44e"
        android:layout_width="253dp"
        android:layout_height="44dp"
        tools:layout_editor_absoluteY="93dp"
        tools:layout_editor_absoluteX="122dp"
        android:id="@+id/textView2"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:foreground="@android:color/transparent"
        android:layout_marginTop="16dp"
        android:layout_below="@+id/imageView"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />


</RelativeLayout>
