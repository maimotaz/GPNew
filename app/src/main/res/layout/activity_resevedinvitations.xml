<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_resevedinvitations"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@drawable/background"
    tools:context="com.example.dalal.gpnew.resevedinvitations">


    <TextView
        android:onClick="Back"
        android:text=""
        android:background="@drawable/zwara_word"
        android:layout_width="200sp"
        android:layout_height="50sp"
        android:id="@+id/textView26"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:text=""
        android:background="@drawable/arow"
        android:layout_width="40sp"
        android:layout_height="35sp"
        android:id="@+id/textView27"
        android:layout_alignBottom="@+id/textView26"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="13dp" />

    <TextView
        android:text=""
        android:layout_width="40sp"
        android:background="@drawable/person"
        android:layout_height="35sp"
        android:id="@+id/textView28"
        android:layout_alignBaseline="@+id/textView27"
        android:layout_alignBottom="@+id/textView27"
        android:layout_toRightOf="@+id/textView26"
        android:layout_toEndOf="@+id/textView26"
        android:layout_marginLeft="10dp"
        android:layout_marginStart="10dp" />
    <Button
        android:text=""
        android:background="@drawable/open"
        android:layout_width="90sp"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/button"
        android:layout_alignBottom="@+id/textView" />



    <Button
        android:text=""
        android:layout_width="90sp"
        android:background="@drawable/open"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView3"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/button2"
        android:layout_alignBottom="@+id/textView3" />


    <TextView
        android:text="دعوة مها"
        android:textAlignment="center"
        android:textColor="#8B4513"
        android:textSize="20sp"
        android:layout_width="400sp"
        android:background="@drawable/picture3"
        android:layout_height="50sp"
        android:id="@+id/textView3"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textStyle="normal|bold" />

    <TextView
        android:text="دعوة سارة"
        android:onClick="resev1"
        android:textAlignment="center"
        android:textColor="#8B4513"
        android:textSize="20sp"
        android:layout_width="400sp"
        android:background="@drawable/picture3"
        android:layout_height="50sp"
        android:id="@+id/textView"
        android:layout_above="@+id/textView3"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="27dp"
        android:textStyle="normal|bold" />

    <TextView
        android:text="دعوات استقبلتها"
        android:textColor="#D98518"
        android:textSize="40sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView2"
        android:layout_below="@+id/textView26"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="19dp" />


</RelativeLayout>
