<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_userslist"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@drawable/background"
    tools:context="com.example.dalal.gpnew.userslist">

    <ImageView
        android:layout_width="227dp"
        android:layout_height="49dp"
        android:background="@drawable/zwarahh"
        android:id="@+id/imageView"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />







    <Button
        android:text=""
        android:background="@drawable/add_user"
        android:layout_width="wrap_content"
        android:layout_height="60sp"
        android:id="@+id/button15"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="27dp"
        />

    <Button
        android:text=""
        android:background="@drawable/pasket"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button16"
        android:layout_alignTop="@+id/editText10"
        android:layout_alignBottom="@+id/editText10"
        />

    <Button
        android:text=""
        android:background="@drawable/pasket"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button17"
        android:layout_alignTop="@+id/editText11"
        android:layout_alignBottom="@+id/editText11"
        />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="50sp"
        android:background="@drawable/picture3"
        android:inputType="textPersonName"
        android:textSize="20sp"
        android:textColor="#8C5812"
        android:text="     محمد"
        android:ems="10"
        android:id="@+id/editText11"
        android:layout_marginTop="14dp"
        android:layout_below="@+id/button16"
        android:layout_centerHorizontal="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="50sp"
        android:background="@drawable/picture3"
        android:inputType="textPersonName"
        android:textSize="20sp"
        android:textColor="#8C5812"
        android:text="      سارة"
        android:ems="10"
        android:id="@+id/editText10"
        android:layout_marginTop="14dp"
        android:layout_below="@+id/textView41"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:text="قائمة المستخدمين"
        android:textColor="#D98518"
        android:textSize="30sp"
        android:layout_width="wrap_content"
        android:layout_height="50sp"
        android:layout_marginTop="22dp"
        android:id="@+id/textView41"
        android:layout_below="@+id/searchView2"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <SearchView
        android:layout_width="match_parent"
        android:background="#FFEEE3D6"
        android:layout_height="50sp"
        android:id="@+id/searchView2"
        android:layout_marginTop="57dp"
        android:layout_below="@+id/imageView"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:text="بحث"
        android:textColor="#D98518"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView40"
        android:textStyle="normal|bold"
        android:textSize="30sp"
        android:layout_below="@+id/imageView"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />


</RelativeLayout>
