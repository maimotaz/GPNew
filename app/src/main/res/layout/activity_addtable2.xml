<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_addtable2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@drawable/back"
    tools:context="com.example.dalal.gpnew.addtable">

    <ImageButton
        android:onClick="Back"
        android:layout_width="50dp"
        android:layout_height="70dp"
        android:background="@drawable/arow"
        android:id="@+id/imageButton2"
        android:layout_alignBottom="@+id/imageView"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="200sp"
        android:layout_height="50sp"
        android:background="@drawable/zwara_word"
        android:id="@+id/imageView"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />


    <ImageButton
        android:onClick="profile"
        android:layout_width="80dp"
        android:layout_height="71dp"
        tools:layout_editor_absoluteY="71dp"
        tools:layout_editor_absoluteX="0dp"
        android:id="@+id/imageButton3"
        android:scaleType="fitXY"
        android:background="@drawable/pers"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignBottom="@+id/imageView" />




    <TextView
        android:text="عدد المقاعد"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteY="255dp"
        tools:layout_editor_absoluteX="219dp"
        android:id="@+id/textView8"
        android:textColor="#906532"
        android:layout_below="@+id/spinner3"
        android:layout_alignRight="@+id/textView9"
        android:layout_alignEnd="@+id/textView9" />

    <Spinner
        android:layout_width="99dp"
        android:layout_height="59dp"
        tools:layout_editor_absoluteY="301dp"
        tools:layout_editor_absoluteX="49dp"
        android:id="@+id/spinner"
        android:layout_below="@+id/spinner3"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />





    <ImageButton
        android:layout_width="95dp"
        android:layout_height="50dp"
        app:srcCompat="@drawable/addtable"
        android:id="@+id/imageButton"
        android:scaleType="fitXY"
        tools:background="@android:color/transparent"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="22dp"
        android:layout_marginStart="22dp"
        android:layout_marginBottom="45dp" />

    <ImageButton
        android:background="@drawable/canceltable"
        tools:layout_editor_absoluteY="421dp"
        tools:layout_editor_absoluteX="246dp"
        android:id="@+id/imageButton14"
        android:scaleType="fitXY"
        android:layout_width="95dp"
        android:layout_height="50dp"
        android:layout_alignTop="@+id/imageButton"
        android:layout_alignRight="@+id/textView8"
        android:layout_alignEnd="@+id/textView8" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="286dp"
        android:layout_height="34dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/imageView"
        android:layout_marginTop="20dp"
        android:foreground="@android:color/transparent"
        android:text="إضافة طاولة جديدة"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textColor="#eeb44e"
        android:textSize="30sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="49dp"
        tools:layout_editor_absoluteY="89dp" />

    <Spinner
        android:layout_width="99dp"
        android:layout_height="59dp"
        tools:layout_editor_absoluteY="169dp"
        tools:layout_editor_absoluteX="49dp"
        android:id="@+id/spinner2"
        android:layout_above="@+id/textView9"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Spinner
        android:layout_width="99dp"
        android:layout_height="59dp"
        tools:layout_editor_absoluteY="236dp"
        tools:layout_editor_absoluteX="49dp"
        android:id="@+id/spinner3"
        android:layout_alignBaseline="@+id/textView9"
        android:layout_alignBottom="@+id/textView9"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:text="رقم الطاولة"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteY="187dp"
        tools:layout_editor_absoluteX="220dp"
        android:id="@+id/textView"

        android:textColor="#906532"
        android:layout_marginBottom="33dp"
        android:layout_above="@+id/textView9"
        android:layout_alignLeft="@+id/textView8"
        android:layout_alignStart="@+id/textView8" />

    <TextView
        android:text="حالة الطاولة"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteY="324dp"
        tools:layout_editor_absoluteX="213dp"
        android:id="@+id/textView9"
        android:textColor="#906532"
        android:layout_marginRight="16dp"
        android:layout_marginEnd="16dp"
        android:layout_centerVertical="true"
        android:layout_alignRight="@+id/textView2"
        android:layout_alignEnd="@+id/textView2" />

</RelativeLayout>
