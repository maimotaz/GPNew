<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_addtable"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@drawable/back"
    tools:context="com.example.dalal.gpnew.addfrind">


    <ImageButton
        android:onClick="Back"
        android:layout_width="50dp"
        android:layout_height="70dp"
        android:background="@drawable/arow"
        android:id="@+id/imageButton2"
        android:layout_alignBottom="@+id/imageView"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="200sp"
        android:layout_height="50sp"
        android:background="@drawable/zwara_word"
        android:id="@+id/imageView"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />


    <ImageButton
        android:onClick="profile"
        android:layout_width="80dp"
        android:layout_height="71dp"
        tools:layout_editor_absoluteY="71dp"
        tools:layout_editor_absoluteX="0dp"
        android:id="@+id/imageButton3"
        android:scaleType="fitXY"
        android:background="@drawable/pers"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignBottom="@+id/imageView" />



    <ImageView
        android:layout_width="1100sp"
        android:layout_height="50sp"
       android:background="@drawable/picture3"
        tools:layout_editor_absoluteY="203dp"
        tools:layout_editor_absoluteX="0dp"
        android:id="@+id/imageView10"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="207dp"
        android:layout_height="38dp"
        android:layout_above="@+id/imageView10"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginBottom="26dp"
        android:foreground="@android:color/transparent"
        android:text="اكتب اسم المستخدم للبحث"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        android:textColor="#E29302"
        android:textSize="24sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="165dp"
        tools:layout_editor_absoluteY="73dp" />

    <TextView
        android:text="محمد"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteY="226dp"
        tools:layout_editor_absoluteX="304dp"
        android:id="@+id/textView5"
        android:textAllCaps="false"
        android:textColor="#463825"
        android:layout_centerVertical="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="20dp"
        android:layout_marginEnd="20dp" />


    <TextView
        android:id="@+id/textView4"
        android:layout_width="107dp"
        android:layout_height="44dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/imageView"
        android:foreground="@android:color/transparent"
        android:text="بحث"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        android:textColor="#E29302"
        android:textSize="24sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="261dp"
        tools:layout_editor_absoluteY="111dp" />

    <SearchView
        tools:layout_editor_absoluteY="111dp"
        tools:layout_editor_absoluteX="27dp"
        android:orientation="horizontal"
        android:layout_width="289dp"
        android:layout_height="35dp"
        android:background="#F0EDE6"
        android:backgroundTint="#F0EDE6"
        android:layout_alignBottom="@+id/textView4"
        android:layout_alignRight="@+id/imageView"
        android:layout_alignEnd="@+id/imageView">

    </SearchView>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/aadd"
        android:id="@+id/imageView5"
        android:layout_alignRight="@+id/imageView2"
        android:layout_alignEnd="@+id/imageView2"
        android:layout_alignBottom="@+id/textView5"
        android:layout_alignTop="@+id/textView5" />

</RelativeLayout>
