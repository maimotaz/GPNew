<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_homeagent"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@drawable/background"
    tools:context="com.example.dalal.gpnew.homeagent">

    <ImageButton
        android:onClick="Back"
        android:layout_width="50dp"
        android:layout_height="70dp"
        android:background="@drawable/arow"
        android:id="@+id/imageButton2"
        android:layout_alignBottom="@+id/imageView"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="200sp"
        android:layout_height="50sp"
        android:background="@drawable/zwara_word"
        android:id="@+id/imageView"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />


    <ImageButton
        android:onClick="profile"
        android:layout_width="80dp"
        android:layout_height="71dp"
        tools:layout_editor_absoluteY="71dp"
        tools:layout_editor_absoluteX="0dp"
        android:id="@+id/imageButton3"
        android:scaleType="fitXY"
        android:background="@drawable/pers"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignBottom="@+id/imageView" />


    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/imageButton6"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="34dp"
        android:text="عميل المطعم"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textColor="#eeb44e"
        android:textSize="30sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="123dp"
        tools:layout_editor_absoluteY="81dp" />


    <ImageButton
        android:onClick="unbook"
        android:background="@drawable/tables"
        android:id="@+id/imageButton10"
        android:elevation="0dp"
        android:layout_height="177dp"
        android:layout_width="384dp"
        android:scaleType="fitXY"
        tools:layout_editor_absoluteY="322dp"
        tools:layout_editor_absoluteX="0dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="23dp" />

    <ImageButton
        android:onClick="book"
        android:background="@drawable/bookedtables"
        android:id="@+id/imageButton6"
        android:elevation="0dp"
        android:scaleType="fitXY"
        android:layout_width="384dp"
        tools:layout_editor_absoluteY="145dp"
        tools:layout_editor_absoluteX="0dp"
        android:layout_height="177dp"
        android:layout_above="@+id/imageButton10"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

</RelativeLayout>
