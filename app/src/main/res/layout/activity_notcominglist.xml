<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_notcominglist"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@drawable/background"
    tools:context="com.example.dalal.gpnew.notcominglist">


    <TextView
        android:text=""
        android:layout_width="40sp"
        android:background="@drawable/person"
        android:layout_height="35sp"
        android:id="@+id/textView28"
        android:layout_alignBaseline="@+id/textView27"
        android:layout_alignBottom="@+id/textView27"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:text="نورة: اسفة مرة ثانية"
        android:textColor="#8C5812"
        android:textSize="20sp"
        android:textAlignment="center"
        android:layout_width="1000sp"
        android:layout_height="50sp"
        android:background="@drawable/picture3"
        android:layout_marginTop="25dp"
        android:id="@+id/textView18"
        android:layout_below="@+id/textView17"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textStyle="normal|bold" />

    <TextView
        android:text="غير الحاضرين"
        android:textColor="#D98518"
        android:textSize="40sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView13"
        android:layout_below="@+id/textView28"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="31dp" />

    <TextView
        android:text=""
        android:background="@drawable/zwara_word"
        android:layout_width="200sp"
        android:layout_height="50sp"
        android:id="@+id/textView26"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:text="دانة: اسفة ما اقدر اجي"
        android:textAlignment="center"
        android:background="@drawable/picture3"
        android:layout_width="1000sp"
        android:layout_height="50sp"
        android:textColor="#8C5812"
        android:textSize="20sp"
        android:layout_marginTop="21dp"
        android:id="@+id/textView17"
        android:layout_below="@+id/textView13"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textStyle="normal|bold" />

    <TextView
        android:text="دلال: عندي اختبار و مشروع"
        android:textAlignment="center"
        android:background="@drawable/picture3"
        android:layout_width="1000sp"
        android:layout_height="50sp"
        android:textColor="#8C5812"
        android:textSize="20sp"
        android:layout_marginTop="28dp"
        android:id="@+id/textView7"
        android:layout_below="@+id/textView18"
        android:textStyle="normal|bold"/>

</RelativeLayout>
