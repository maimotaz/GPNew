<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_notcominglist"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@drawable/background"
    tools:context="com.example.dalal.gpnew.notcominglist">

    <ImageButton
        android:onClick="Back"
        android:layout_width="50dp"
        android:layout_height="70dp"
        android:background="@drawable/arow"
        android:id="@+id/imageButton2"
        android:layout_alignBottom="@+id/imageView"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="200sp"
        android:layout_height="50sp"
        android:background="@drawable/zwara_word"
        android:id="@+id/imageView"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />


    <ImageButton
        android:onClick="profile"
        android:layout_width="80dp"
        android:layout_height="71dp"
        tools:layout_editor_absoluteY="71dp"
        tools:layout_editor_absoluteX="0dp"
        android:id="@+id/imageButton3"
        android:scaleType="fitXY"
        android:background="@drawable/pers"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignBottom="@+id/imageView" />

    <TextView
        android:text="نورة: اسفة مرة ثانية"
        android:textColor="#8C5812"
        android:textSize="20sp"
        android:textAlignment="center"
        android:layout_width="1000sp"
        android:layout_height="50sp"
        android:background="@drawable/picture3"
        android:layout_marginTop="25dp"
        android:id="@+id/textView18"
        android:layout_below="@+id/textView17"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textStyle="normal|bold" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageView"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="18dp"
        android:text="غير الحاضرين"
        android:textColor="#D98518"
        android:textSize="30sp"
        android:textStyle="bold" />



    <TextView
        android:text="دانة: اسفة ما اقدر اجي"
        android:textAlignment="center"
        android:background="@drawable/picture3"
        android:layout_width="1000sp"
        android:layout_height="50sp"
        android:textColor="#8C5812"
        android:textSize="20sp"
        android:layout_marginTop="21dp"
        android:id="@+id/textView17"
        android:layout_below="@+id/textView13"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textStyle="normal|bold" />

    <TextView
        android:text="دلال: عندي اختبار و مشروع"
        android:textAlignment="center"
        android:background="@drawable/picture3"
        android:layout_width="1000sp"
        android:layout_height="50sp"
        android:textColor="#8C5812"
        android:textSize="20sp"
        android:layout_marginTop="28dp"
        android:id="@+id/textView7"
        android:layout_below="@+id/textView18"
        android:textStyle="normal|bold"/>

</RelativeLayout>
