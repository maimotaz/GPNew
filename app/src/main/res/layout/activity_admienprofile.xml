<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_admienprofile"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@drawable/background"
    tools:context="com.example.dalal.gpnew.admienprofile">


    <ImageView
        android:layout_width="200sp"
        android:layout_height="50sp"
        android:background="@drawable/zwara_word"
        android:id="@+id/imageView"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />


    <ImageView
        android:layout_width="278dp"
        android:layout_height="692dp"
        android:background="@drawable/pro"
        tools:layout_editor_absoluteY="57dp"
        tools:layout_editor_absoluteX="0dp"
        android:id="@+id/imageView4"
        android:layout_below="@+id/imageView"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageView
        android:layout_width="68dp"
        android:background="@drawable/usr"
        tools:layout_editor_absoluteY="298dp"
        tools:layout_editor_absoluteX="312dp"
        android:id="@+id/imageView7"
        android:layout_height="68dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="84dp" />

    <ImageView
        android:layout_width="68dp"
        android:background="@drawable/nn"
        tools:layout_editor_absoluteY="230dp"
        tools:layout_editor_absoluteX="316dp"
        android:layout_height="68dp"
        android:layout_above="@+id/imageView7"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:id="@+id/imageView8" />


    <EditText
        android:layout_width="155dp"
        android:inputType="textPersonName"
        android:text="اسم "
        android:ems="10"
        tools:layout_editor_absoluteY="238dp"
        tools:layout_editor_absoluteX="173dp"
        android:id="@+id/editText"
        android:layout_height="55dp"
        android:textColor="#7a5a23"
        android:layout_alignTop="@+id/imageView8"
        android:layout_alignRight="@+id/imageView"
        android:layout_alignEnd="@+id/imageView" />


    <EditText
        android:inputType="textPersonName"
        android:text="اسم المستخدم"
        android:ems="10"
        tools:layout_editor_absoluteY="302dp"
        tools:layout_editor_absoluteX="169dp"
        android:id="@+id/editText4"
        android:layout_width="155dp"
        android:layout_height="55dp"
        android:textColor="#7a5a23"
        android:layout_below="@+id/imageView8"
        android:layout_alignLeft="@+id/editText"
        android:layout_alignStart="@+id/editText" />

    <ImageView
        android:layout_width="158dp"
        android:layout_height="153dp"
        tools:layout_editor_absoluteY="69dp"
        tools:layout_editor_absoluteX="80dp"
        android:id="@+id/imageView5"
        android:background="@drawable/profile"
        android:layout_below="@+id/imageView"
        android:layout_alignLeft="@+id/imageView4"
        android:layout_alignStart="@+id/imageView4"
        android:layout_marginTop="19dp" />

    <ImageButton
        android:onClick="Back"
        android:layout_width="240dp"
        android:layout_height="42dp"
        tools:layout_editor_absoluteY="445dp"
        tools:layout_editor_absoluteX="128dp"
        android:scaleType="fitXY"
        android:background="@drawable/exit"
        android:layout_marginBottom="20dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="13dp"
        android:layout_marginEnd="13dp" />

</RelativeLayout>
